{
    "name": "andrey/smstraffic-for-laravel",
    "description": "SmsTraffic Client for Laravel",
    "type": "library",
    "keywords": ["smstraffic", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Dmitrakovich",
            "email": "dmitrievich.a.d@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/notifications": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0",
        "guzzlehttp/guzzle": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Illuminate\\Notifications\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Illuminate\\Notifications\\SmsTrafficChannelServiceProvider"
            ],
            "aliases": {
                "SmsTraffic": "Illuminate\\Notifications\\Facades\\SmsTraffic"
            }
        }
    }
}
